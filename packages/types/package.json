{
  "name": "@cessnetwork/types",
  "version": "0.1.1",
  "description": "Type definitions for CESS SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias",
    "dev": "tsc -p tsconfig.json --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "cess",
    "blockchain",
    "storage",
    "substrate",
    "web3",
    "sdk",
    "types"
  ],
  "author": "CESS Labs",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cessproject/api.git"
  },
  "bugs": {
    "url": "https://github.com/cessproject/api/issues"
  },
  "homepage": "https://github.com/cessproject/api#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@polkadot/api": "16.4.6",
    "@polkadot/types": "16.4.6",
    "@polkadot/util": "13.5.6",
    "@polkadot/util-crypto": "^13.5.6"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "tsc-alias": "^1.8.16"
  }
}